DIRS = $ffff
OUTS = $0000

i VAR Byte
j VAR Byte
k VAR Byte
w VAR Word
p VAR Word
n VAR Byte

pattern VAR Word

cycle VAR Byte
repeat VAR Byte
delay VAR Byte


main:

READ program, n

FOR k = 0 TO n-1
  READ program + 1 + (k << 1), Word pattern
  'DEBUG DEC program, CR
  GOSUB DoPattern
NEXT
GOTO main







DoPattern:
READ pattern + 0, cycle
READ pattern + 1, repeat
READ pattern + 2, delay
p = pattern + 3
cycle = (cycle - 1) * 2

FOR i = 1 TO repeat
  FOR j = 0 TO cycle STEP 2
    READ p + j, Word w
    OUTS = w
    'DEBUG DEC j, " ", BIN w, CR
  PAUSE delay
  NEXT
NEXT

